@using CRMEngSystem.Models.ViewModels.Order
@using CRMEngSystem.Data.Enums
@model OrderStatusViewModel

<head>
    <style>
        .status-container{
            margin-top: 30px;
            max-width: max-content;
        }

        .status-container-row{
            display: flex;
            flex-direction: row;
            align-items: center;
            gap: 30px;
            max-width: max-content;
        }

            .status-container-row svg{
                height: 80px;
            }

        .status-container-row-data{
            display: flex;
            flex-direction: column;
            text-align: left;
            gap: 5px;
            font-size: 14px;
            font-weight: 800;
        }

        .status-container-row-data-active{
            color: #4962E9;
        }

        .status-container-row-data-completed{
            color: #01D36E;
        }

        .status-container-row-data span:nth-child(2) {
            font-weight: normal;
            color: #E1E2F3;
            font-size: 12px;
        }
    </style>
</head>
