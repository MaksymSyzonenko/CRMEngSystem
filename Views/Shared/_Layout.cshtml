@inject IRepositoryFactory _repositoryFactory

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Stimex CRM</title>
    <link rel="icon" type="image/svg+xml" href="/icons/Logo_Small.svg">
