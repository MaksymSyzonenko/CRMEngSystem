            }
            @if (user!.Contact.Image != null)
            {
                @if (user!.Contact.Image.Bytes == null)
                {
                    <a style="height: 20px; display: flex; justify-content: center; align-items: center;" href="@Url.ActionLink("ContactDetails", "ContactDetails", new { EntityId = user.ContactId })">
                        <img src="~/images/contact/Contact_Empty_Image.png">
                    </a>
                }
                else
                {
                    <a style="height: 20px; display: flex; justify-content: center; align-items: center;" href="@Url.ActionLink("ContactDetails", "ContactDetails", new { EntityId = user.ContactId })">
                        <img src="data:image/jpeg;base64,@(Convert.ToBase64String(user.Contact.Image.Bytes!))" />
                    </a>
                }
            }
            else
            {
                <a style="height: 20px; display: flex; justify-content: center; align-items: center;" href="@Url.ActionLink("ContactDetails", "ContactDetails", new { EntityId = user.ContactId })">
