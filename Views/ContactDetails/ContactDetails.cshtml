@using CRMEngSystem.Models.ViewModels.Contact
@using CRMEngSystem.Models.ViewModels.Notify
@using CRMEngSystem.Models.ViewModels.Confirm
@model ContactDetailsViewModel

<head>
    <style>
        .tab-container {
            display: flex;
            flex-direction: row;
            width: 100%;
            gap: 10px;
        }

        .details-container {
            background-color: white;
            border-radius: 5px;
            padding: 10px;
            color: #595586;
            width: 100%;
        }

        .details-container-header {
            display: flex;
            flex-direction: row;
            gap: 20px;
            font-size: 14px;
            font-weight: bold;
            align-items: center;
            width: 100%;
        }

        .details-container img{
            border-radius: 50%;
            height: 50px;
            width: 50px;
        }

        .details-container-data{
            display: flex;
            flex-direction: column;
            margin-top: 15px;
            gap: 10px;
            font-size: 12px;
        }

        .details-container-data-row {
            display: flex;
            border-radius: 5px;
            width: 100%;
            padding: 5px 10px;
            align-items: center;
            justify-content: space-between;
        }

        .details-container-data-row-filled {
            display: flex;
            border-radius: 5px;
            width: 100%;
            padding: 5px 10px;
            align-items: center;
            justify-content: space-between;
            background-color: #E1E2F3;
        }

        .details-container-data span:last-child{
            text-align: right;
        }

        .details-container-data span:first-child{
            font-weight: bold;
            width: 250px;
        }

        .func-button-container{
            display: flex;
            flex-direction: row;
            margin-left: auto;
            align-items: center;
            gap: 5px;
        }

        .btn-default:hover path {
                stroke: #4962E9;
            }

        .trash-bucket:hover path{
            fill: #FF705C;
        }

        .right-details-container{
            width: 500px;
            color: #595586;
            display: flex;
            gap: 10px;
            flex-direction: column;
        }

        .diagram-container{
            background-color: white;
            border-radius: 5px;
            width: 100%;
            height: 35%;
            padding: 10px 10px 10px 0;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: row;
        }

        .socialnetworks-container{
            background-color: white;
            border-radius: 5px;
            width: 100%;
            height: 65%;
            padding: 30px 20px;
            display: flex;
            flex-direction: row;
            gap: 15px;
        }

        .network-link{
            border-bottom: 2px solid #E1E2F3;
            padding: 3px;
            font-weight: 800;
            font-size: 12px;
            color: #595586;
            display: flex;
            justify-content: space-between;
            align-items: center;
            text-decoration: none;
            width: 100%;
        }

        .network-link span{
            text-align: left;
        }

        .network-link svg{
            text-align: right;
        }

            .network-link:hover  {
                color: #4962E9;
                border-bottom: 2px solid #4962E9;
            }

            .network-link:hover svg path{
                fill: #4962E9;
            }

        .left{
            width: 100%;
            display: flex;
            flex-direction: column;
            gap: 25px;
        }

        .right {
            width: 100%;
            display: flex;
            flex-direction: column;
            gap: 25px;
        }

        .diagram{
            width: 115px;
            height: 115px;
            padding-bottom: 10px;
        }

        .diagram-labels{
            display: flex;
            flex-direction: column;
            gap: 20px;
        }

        .diagram-label{
            font-size: 12px;
            color: #595586;
            display: flex;
            flex-direction: row;
            gap: 10px;
            align-items: center;
        }

        .labl
        {
            width: 130px;
        }

        .card-data-link:hover span {
            color: #4962E9;
        }

        .card-data-link {
            text-decoration: none;
            font-size: 12px;
            color: #595586;
            
        }

    </style>
</head>
<body>
    @if (TempData["ConfirmModal"] != null && (bool)TempData["ConfirmModal"]!)
    {
        @await Html.PartialAsync("_ConfirmModal", new ConfirmModalViewModel { NotifyText = (string)TempData["NotifyText"]!, ConfirmActionName = "ConfirmModal", ControllerName = "ContactRemove", RouteValues = new { EntityId = Model.EntityId} })
    }
    @if (TempData["NotifyModal"] != null && (bool)TempData["NotifyModal"]!)
    {
        @await Html.PartialAsync("_NotifyModal", new NotifyModalViewModel { NotifyText = (string)TempData["NotifyText"]!, IsError = false })
    }
    else if (TempData["ErrorNotifyModal"] != null && (bool)TempData["ErrorNotifyModal"]!)
    {
        @await Html.PartialAsync("_NotifyModal", new NotifyModalViewModel { NotifyText = (string)TempData["NotifyText"]!, IsError = true })
    }
    <div class="page-container">
        @await Html.PartialAsync("_ContactMenuTab", Model)
        <div class="tab-container">
            <div class="details-container">
                <div class="details-container-header">
                    @if (Model.Contact.Image!.Length == 0)
                    {
                        <img src="~/images/contact/Contact_Empty_Image.png">
                    }
                    else
                    {
